<root>
	<folder path="c:\Documents and Settings\All Users\Start Menu" clean="true">
		<link name="Notepad++">
			<target>%TOOLS_DIR%\Notepad++\notepad++.exe</target>
		</link>

		<link name="Mozilla Firefox">
			<target>%PROGRAMFILES%\Mozilla Firefox\Firefox.exe</target>
